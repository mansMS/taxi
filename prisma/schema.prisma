generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}
datasource db {
  provider = "postgresql"
}

model Car {
  id            String    @id @default(uuid())
  image         String
  name          String
  color         String
  number        String
  routes        String[]
  description   String
  driverId      String
  createdAt     DateTime  @default(now()) @map("created_at")
  updatedAt     DateTime  @updatedAt @map("updated_at")

  @@map("car")
}